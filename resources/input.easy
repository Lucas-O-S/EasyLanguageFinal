programa
{
numero i, soma;
booleano flag;
texto nome;
inteiro a;
caractere v;

    a = 2;
    v = 'c';
    leia(nome);
    escreva(nome);
    soma = 0;
    flag = verdadeiro;

    vetor texto meuvetor [2];

    meuvetor[2] = "A";

    para (i de 1 <= 10 passo 2) faca {
        escreva(i);
    }

    se (soma < 100 e nao flag == true) {
        escreva(soma);
    } senao {
        escreva(0);
    }

    enquanto (soma < 100 ou flag == falso) {
        soma = soma + 10;
    }
}
fimprog;
